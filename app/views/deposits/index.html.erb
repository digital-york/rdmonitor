<p id="notice"><%= notice %></p>

<h1>Listing deposits</h1>

<table>
  <thead>
    <tr>
      <th>Uuid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  <% @deposits.each do |upload| %>
      <tr>
        <td><%= upload %></td>
        <td><%= link_to 'Show', upload %></td>
        <td><%= link_to 'Edit', edit_deposit_path(upload) %></td>
        <td><%= link_to 'Destroy', upload, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
  <% end %>
  </tbody>

</table>

<br>

<%= link_to 'New deposit', new_deposit_path %>
